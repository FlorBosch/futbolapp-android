<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ar.com.futbolapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ui.FutbolAppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.auth0.lock.client-id"
            android:value="VE41FsxEbp68vgfIEBHPvwtX9wLud1OE" />
        <meta-data
            android:name="com.auth0.lock.domain-url"
            android:value="imgaray.auth0.com" />

        <!-- Auth0 Lock End -->
        <activity
            android:name=".ui.activity.UserDashboardActivity"
            android:label="@string/title_activity_user_dashboard" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
